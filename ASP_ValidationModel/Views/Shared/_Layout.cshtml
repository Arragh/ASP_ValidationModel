<!DOCTYPE html>

<html>
<head>
    <!--Metatags-->
    @await Html.PartialAsync("MetatagsPartial")
</head>
<body bgcolor="darkgrey">

    <!--Header-->
    @await Html.PartialAsync("HeaderPartial")

    <div align="center">
        <table border="1" width="1200" height="800" bgcolor="lightblue" cellpadding="0" cellspacing="0">
            <tr>
                <td>@RenderBody()</td>
            </tr>
        </table>
    </div>

    <!--Bottom-->
    @await Html.PartialAsync("BottomPartial")

</body>
</html>
